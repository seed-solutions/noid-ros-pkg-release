<launch>

  <!--joint_state_publisher translating lifter angles to lifter position-->
  <node name="joint_state_publisher_with_lifter" pkg="noid_typef_moveit_config" type="joint_state_publisher_with_lifter.py"  output="screen">
  </node>

  <!--/rarm_controller-->
  <node name="rarm_controller" pkg="noid_typef_moveit_config" type="joint_trajectory_action_server.py"  output="screen">
    <rosparam command="load" file="$(find noid_typef_moveit_config)/config/controllers.yaml" />
  </node>

  <!--/larm_controller-->
  <node name="larm_controller" pkg="noid_typef_moveit_config" type="joint_trajectory_action_server.py"  output="screen">
    <rosparam command="load" file="$(find noid_typef_moveit_config)/config/controllers.yaml" />
  </node>

  <!--/torso_controller-->
  <node name="torso_controller" pkg="noid_typef_moveit_config" type="joint_trajectory_action_server.py"  output="screen">
    <rosparam command="load" file="$(find noid_typef_moveit_config)/config/controllers.yaml" />
  </node>

  <!--/head_controller-->
  <node name="head_controller" pkg="noid_typef_moveit_config" type="joint_trajectory_action_server.py"  output="screen">
    <rosparam command="load" file="$(find noid_typef_moveit_config)/config/controllers.yaml" />
  </node>

  <!--/lifter_controller-->
  <node name="lifter_controller" pkg="noid_typef_moveit_config" type="lifter_controller.py"  output="screen">
  </node>
</launch>
